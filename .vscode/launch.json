{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "request": "attach",
            "name": "Attach by Process ID",
            "processId": "${command:PickJavaProcess}"
        },
        {
            "type": "java",
            "name": "Attach",
            "request": "attach",
            "hostName": "localhost",
            "port": "9091"
        },
        {
            "type": "java",
            "name": "Launch Component Generator",
            "request": "launch",
            "mainClass": "ComponentGenerator.CommandLineHandler",
            "projectName": "compgen",
            "cwd": "${workspaceFolder}/ComponentGenerator"
        },
        {
            "type": "java",
            "name": "Launch TestRunner",
            "request": "launch",
            "mainClass": "org.genevaers.mr91comparisons.TestRunner",
            "projectName": "petestframework",
            "cwd": "${workspaceFolder}/PETestFramework"
        },
        {
            "type": "java",
            "name": "Launch TestFramework Menu",
            "request": "launch",
            "mainClass": "org.genevaers.testframework.CommandLineHandler",
            "projectName": "petestframework",
            "cwd": "${workspaceFolder}/PETestFramework",
            "args": "-menu"
        },
        {
            "type": "java",
            "name": "Launch TestFramework All",
            "request": "launch",
            "mainClass": "org.genevaers.testframework.CommandLineHandler",
            "projectName": "petestframework",
            "cwd": "${workspaceFolder}/PETestFramework"
        },
        {
            "type": "java",
            "name": "Launch RCG from playpen",
            "request": "launch",
            "mainClass": "org.genevaers.rcapps.Runner",
            "projectName": "rcapps",
            "cwd": "${workspaceFolder}/../../../RunControlPlayPen"
        },
        {
            "type": "java",
            "name": "Launch RCG/RCA from WEReports",
            "request": "launch",
            "mainClass": "org.genevaers.rcapps.Runner",
            "projectName": "rcapps",
            "cwd": "${workspaceFolder}/../../../WEReports/RCAReport"
        }
    ]
}