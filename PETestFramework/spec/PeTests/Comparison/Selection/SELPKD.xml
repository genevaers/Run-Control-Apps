<PMSpec>
	<Name>SELPKD</Name>
	<Title>Selection via Comparisons</Title>
	<Description>Select via ompare different source types to packed.</Description>
        <Category>PeTests/Comparison/Selection</Category>
	<CQ></CQ>
	<Note></Note>
	<Tests>
		<Test>
			<Name>PKDBEG</Name>
			<Header>LessThan</Header>
			<Description>Select with BEGINS</Description>
			<Source>XML</Source>
			<DecimalPlaces>0</DecimalPlaces>
			<XMLfiles>
				<XMLfile>
					<Name>packedSELECT.xml</Name>
					<Replacements>
						<Replacement>
							<Replace>%SELSTMNT%</Replace>
                            <With>SELECTIF({Description} BEGINS_WITH \"4-ve\")</With>
						</Replacement>
					</Replacements>
				</XMLfile>
			</XMLfiles>
			<EventFiles>
				<EventFile>
					<DDName>ALLTYPE</DDName>
					<FileName>ALLTYPE</FileName>
				</EventFile>
			</EventFiles>
			<ExtractFiles>
				<ExtractFile>
                    <SPACE>TRK</SPACE>
                    <PRIMARY>1</PRIMARY>
                    <SECONDARY>10</SECONDARY>
                    <RECFM>VB</RECFM>
                    <LRECL>8192</LRECL>
 				</ExtractFile>
			</ExtractFiles>
            <FormatFiles>
                <FormatFile>
                    <DDName>FMTOUT01</DDName>
                    <SPACE>TRK</SPACE>
                    <PRIMARY>1</PRIMARY>
                    <SECONDARY>1</SECONDARY>
                    <RECFM>VB</RECFM>
                    <LRECL>200</LRECL>
                </FormatFile>
            </FormatFiles>            
			<Result>
				<Success>
				</Success>
			</Result>
        </Test>
